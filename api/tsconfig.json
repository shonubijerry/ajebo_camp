{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "bundler",
    "types": [
    "./worker-configuration.d.ts",
      "@types/node"
    ],
    "noEmit": true,
    "isolatedModules": true,
